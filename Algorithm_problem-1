#include<iostream>
using namespace std;
int main()
{
    cout<<"Bilangan-1:10\n";
    cout<<"Bilangan-2:23\n";
    cout<<"Bilangan-3:15\n";
    if(10 >= 23&&10>=15)
    {
	cout<<"largest number:\n "<<10<<endl;
    } 
    if (23>=10&&23>=15)
    {
    	cout<<"Bilangan terbesar adalah : "<<23<<endl;
	}
	if(15>=10&&15>=23){
		cout<<"Bilangan terbesar adalah :\n "<<15<<endl;
	}
	return 0;
}
