#include<iostream>

using namespace std;

int main(){
	cout<<"=====Mula-mula========="<<endl;
	cout<<"Angka-1 : 3"<<endl;
	cout<<"Angka-2 : 20"<<endl;
	cout<<"Angka-3 : 33"<<endl;
	cout<<"=====Hasil============="<<endl;
	if ( 3%2 == 0){
		cout<< 3 <<": Bilangan Genap\n";
	} else{
		cout<< 3 <<": Bilangan Ganjil\n";
	}
	if ( 20%2 == 0){
		cout<< 20 <<": Bilangan Genap\n";
	} else{
		cout<< 20 <<": Bilangan Ganjil\n";
	}
	if ( 33%2 == 0){
		cout<< 33 <<": Bilangan Genap\n";
	} else{
		cout<< 33 <<": Bilangan Ganjil\n";
	}
	return 0;
	
}
